{
	"name": "lsr/helpers",
	"description": "Laser framework core - Helpers.",
	"version": "0.1.2",
	"scripts": {
		"analyze": "vendor/bin/phpstan analyze",
		"test": "vendor/bin/phpunit"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://packages.laserliga.cz"
		}
	],
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"phpunit/phpunit": "^9.5",
		"phpstan/phpstan": "^1.8",
		"phpstan/extension-installer": "^1.1",
		"phpstan/phpstan-dibi": "^1.0",
		"phpstan/phpstan-nette": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Lsr\\Helpers\\": "src/",
			"Lsr\\Exceptions\\": "Exceptions/"
		},
		"files": [
			"includes/functions.php"
		]
	},
	"authors": [
		{
			"name": "Tomáš Vojík",
			"email": "vojik@wboy.cz"
		}
	],
	"minimum-stability": "dev",
	"require": {
		"php": ">=8.1",
		"nette/utils": "^3.2",
		"gettext/gettext": "^5.6",
		"tracy/tracy": "^2.8",
		"lsr/interfaces": "^0.1",
		"lsr/routing": "^0.1"
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	}
}
