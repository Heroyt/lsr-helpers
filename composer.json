{
	"name": "lsr/helpers",
	"description": "Laser framework core - Helpers.",
	"version": "0.2.13",
	"scripts": {
		"analyze": "vendor/bin/phpstan analyze",
		"test": "vendor/bin/phpunit"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://packages.laserliga.cz"
		}
	],
	"require-dev": {
		"roave/security-advisories": "dev-latest",
		"phpunit/phpunit": "^9.5",
		"phpstan/phpstan": "^1.8",
		"phpstan/extension-installer": "^1.1",
		"phpstan/phpstan-dibi": "^1.0",
		"phpstan/phpstan-nette": "^1.0"
	},
	"autoload": {
		"psr-4": {
			"Lsr\\Helpers\\": "src/",
			"Lsr\\Exceptions\\": "Exceptions/"
		},
		"files": [
			"includes/functions.php"
		]
	},
	"authors": [
		{
			"name": "Tomáš Vojík",
			"email": "vojik@wboy.cz"
		}
	],
	"minimum-stability": "stable",
	"require": {
		"php": ">=8.1",
		"nette/utils": "^4.0",
		"gettext/gettext": "^5.7",
		"tracy/tracy": "^2.9",
		"lsr/interfaces": "^0.2",
		"lsr/routing": "^0.2"
	},
	"config": {
		"allow-plugins": {
			"phpstan/extension-installer": true
		}
	},
	"extra": {
		"branch-alias": {
			"v0.2": "0.2.x-dev",
			"dev-master": "0.2.x-dev",
			"dev-main": "0.2.x-dev"
		}
	}
}
